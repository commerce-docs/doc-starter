{
  "name": "doc-starter",
  "version": "1.0.0",
  "description": "Start your new documentation project by forking this repository.",
  "main": "index.js",
  "scripts": {
    "install": "bundle install",
    "start": "npm-run-all -s clean develop",
    "develop": "npm run build && node scripts/develop.js",
    "clean": "rm -fr _site",
    "build": "npm-run-all -s build:html",
    "build:html": "bundle exec jekyll build --source src -I",
    "build:production": "npm-run-all -s clean && jekyll build --source src --config src/_config.yml,src/_config-production.yml",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magento-devdocs/doc-starter.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/magento-devdocs/doc-starter/issues"
  },
  "homepage": "https://github.com/magento-devdocs/doc-starter#readme",
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "npm-run-all": "^4.1.5"
  }
}
